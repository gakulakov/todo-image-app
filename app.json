{
  "expo": {
    "name": "Todo Image",
    "slug": "reactNativeSelfPosts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/TodoIcon.png",
    "splash": {
      "image": "./assets/SplashTodo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gakulakov.todo-image",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.gakulakov.todo_image",
      "versionCode": 1,
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
